      <li MayRequire="sarg.alphagenes">AG_PsychicSight</li>
      <li MayRequire="vanillaracesexpanded.archon">VRE_SlowMeditation</li>
      <li MayRequire="Danzen.VPE.BiotechIntegration">Gene_Necropath</li>
      <li MayRequire="Danzen.VPE.BiotechIntegration">Gene_Nightstalker</li>
      <li MayRequire="Danzen.VPE.BiotechIntegration">Gene_Puppeteer</li>
      <li MayRequire="Eclair155.VPEBiotechPatches">Gene_Oculist</li>
    </genes>
  </XenotypeDef>

