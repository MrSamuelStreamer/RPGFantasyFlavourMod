    <descriptionShort>A powerful mage created through generations of arranged marriage to enforce inborn magical prowess.</descriptionShort>
    <iconPath>UI/Icons/Xenotypes/Custom/VBE_CustomXenotypeIcon11</iconPath>
    <inheritable>true</inheritable>
    <genes>
      <li MayRequire="sarg.alphagenes">AG_InnatePsylink</li>
